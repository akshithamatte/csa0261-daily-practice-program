#include <stdio.h>
int main() {
    int arr[10], i, max, min;
    printf("Enter 10 integers: ");
    for (i = 0; i < 10; i++) {
        scanf("%d", &arr[i]);
    }
    max = arr[0];
    min = a
    for (i = 1; i < 10; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
        if (arr[i] < min) {
            min = arr[i];
        }
    }
    // Output maximum and minimum
    printf("Maximum element in array is: %d\n", max);
    printf("Minimum element in array is: %d\n", min);

    return 0;
}
